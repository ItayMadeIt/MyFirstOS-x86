{
  "configurations": [
    {
      "name": "Kernel",
      "includePath": [
        "${workspaceFolder}/kernel/include",
        "${workspaceFolder}/kernel/arch/i386",
        "${workspaceFolder}/libk/include",
        "${workspaceFolder}/libc/include"
      ],
      "defines": [
        "_KERNEL",
        "__i386__"
      ],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "gcc-x86",
      "browse": {
        "path": [
          "${workspaceFolder}/kernel/include",
          "${workspaceFolder}/kernel/arch/i386",
          "${workspaceFolder}/libk/include",
          "${workspaceFolder}/libc/include"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ],
  "version": 4
}
